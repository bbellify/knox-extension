{
  "manifest_version": 3,
  "name": "Knox",
  "description": "your web2 password vault",
  "version": "0.1.0",
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["tabs", "scripting"],
  "host_permissions": ["<all_urls>"],
  "icons": {
    "16": "./knox-100.png",
    "32": "./knox-100.png",
    "48": "./knox-100.png",
    "128": "./knox-100.png"
  },
  "action": {
    "default_popup": "popup.html",
    "default_title": "Open"
  }
}
